{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/instill-ai/connector/blob/main/pkg/configLoader/config/destination_connector.json",
  "title": "DestinationConnector",
  "description": "Standard data destination",
  "type": "object",
  "additionalProperties": false,
  "required": ["id", "destination_connector_definition", "connector"],
  "properties": {
    "name": {
      "type": "string",
      "title": "Resource name",
      "description": "The resource name",
      "readOnly": true,
      "ui_order": 0,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "uid": {
      "type": "string",
      "format": "uuid",
      "title": "UUID",
      "description": "The unique UUID",
      "readOnly": true,
      "ui_order": 1,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "id": {
      "type": "string",
      "title": "Name",
      "description": "The destination connector name",
      "pattern": "^[a-z]([a-z0-9-]{0,61}[a-z0-9])?$",
      "minLength": 1,
      "maxLength": 63,
      "ui_order": 2,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "destination_connector_definition": {
      "type": "string",
      "title": "Destination definition resource name",
      "description": "The resource name of the destination definition",
      "ui_order": 3,
      "ui_hidden": true,
      "ui_disabled": true,
      "ui_component": "text"
    },
    "connector": {
      "type": "object",
      "title": "Connector",
      "description": "Destination connector",
      "$ref": "connector.json",
      "additionalProperties": false,
      "ui_order": 4
    }
  }
}
